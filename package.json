{
    "name": "@statox/vector",
    "version": "0.0.2",
    "description": "A 2D Vector library",
    "main": "./dist/index.js",
    "types": "./dist/index.d.js",
    "type": "module",
    "engines": {
        "node": "24.x"
    },
    "scripts": {
        "check": "npm run lint && npm run prettier",
        "prettier": "prettier --check 'src/**/*.ts' 'tests/**/*.ts'",
        "prettier:fix": "prettier -w 'src/**/*.ts' 'tests/**/*.ts'",
        "lint": "eslint",
        "lint:fix": "npm run lint --fix",
        "tests": "node --test",
        "build": "rm -fr dist/ && tsc",
        "watch": "rm -fr dist/ && tsc -w",
        "publish": "npm run build && npm publish --access public"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3.3.1",
        "@eslint/js": "^9.33.0",
        "@types/node": "^24.2.1",
        "@typescript-eslint/eslint-plugin": "^8.39.0",
        "@typescript-eslint/parser": "^8.39.0",
        "eslint": "^9.33.0",
        "eslint-config-prettier": "^10.1.8",
        "prettier": "^3.6.2",
        "typescript": "^5.9.2"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/statox/ts-vector.git"
    },
    "keywords": [
        "vector",
        "math",
        "2d"
    ],
    "author": "statox",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/statox/ts-vector/issues"
    },
    "homepage": "https://github.com/statox/ts-vector#readme"
}
